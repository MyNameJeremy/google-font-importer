{
  "name": "google-font-importer",
  "displayName": "google-font-importer",
  "description": "Makes importing google fonts easier and faster",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MyNameJeremy/google-font-importer"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:google-font-importer.importFont"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "google-font-importer.importFont",
        "title": "Import Google Font"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "node-fetch": "^3.2.10"
  }
}